# create the ramdisk for the OS image construction
# it is a physcal device )
bash -c "if [ `grep -q  /mnt/root /proc/mounts ` ]; then rm -rf /mnt/root ;	fi"
rm -f /mnt/root/nyble_snap.tar*
echo TARGET = /mnt/root
TARGET = /mnt/root
mkdir -p /mnt/root
touch /mnt/nyble_snap.tar
mount -o size=32g -t tmpfs none /mnt/root
touch ramdisk_build_1
# base
mkdir -p /mnt/root/etc/yum.repos.d/
mkdir -p /mnt/root/etc/yum/vars/
echo "8" > /mnt/root/etc/yum/vars/releasever 
echo "x86_64" > /mnt/root/etc/yum/vars/basearch
echo "nameserver 1.1.1.1" > /mnt/root/etc/resolv.conf
yum -y -c OS/rocky9/Nyble-Bootstrap.repo --disablerepo=* \
	--enablerepo=nyble-bootstrap-base \
	--enablerepo=nyble-bootstrap-appstream \
	--enablerepo=nyble-bootstrap-highavailability  \
	--enablerepo=nyble-bootstrap-powertools  \
	--enablerepo=nyble-bootstrap-centosplus  \
	--enablerepo=nyble-bootstrap-extras  \
	--disableplugin=* \
	--installroot=/mnt/root \
	install bash bash-completion vim-minimal iproute \
	iputils rootfiles sudo rpm nano dnf dracut
